[ignore]
.*/node_modules/*
.*/.next/.*
.*/__tests__/*
.*/server/*
.*/utils/jest.js
.*/utils/renderWithIntl.js
.*/lib/withIntl.js
.*/jest.*.js

[include]

[libs]

[lints]
all=error

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null

[options]
all=true
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
esproposal.optional_chaining=enable
