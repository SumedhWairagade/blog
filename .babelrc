{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    "react-intl",
    "markdown-in-js/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.dev"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "inline-dotenv",
        "markdown-in-js/babel"
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.dev"
          }
        ]
      ]
    }
  },
}
