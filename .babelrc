{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        [
          "inline-dotenv",
          {
            "path": ".env.dev"
          }
        ],
        "markdown-in-js/babel",
        "react-intl"
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        "inline-dotenv",
        "markdown-in-js/babel",
        "react-intl"
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        [
          "inline-dotenv",
          {
            "path": ".env.dev"
          }
        ],
        "markdown-in-js/babel",
        "react-intl"
      ]
    }
  }
}
