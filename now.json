{
  "name": "blog",
  "version": 2,
  "alias": [
    "mcansh.blog",
    "www.mcansh.blog"
  ],
  "regions": [
    "all"
  ],
  "builds": [
    {
      "src": "/packages/www/package.json",
      "use": "@now/next@canary",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "/packages/api/*.js",
      "use": "@now/node"
    },
    {
      "src": "packages/www/static/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/packages/api/$1"
    },
    {
      "src": "/manifest.json",
      "methods": [
        "GET"
      ],
      "dest": "/packages/www/static/manifest.json"
    },
    {
      "src": "/feed.json",
      "methods": [
        "GET"
      ],
      "dest": "/packages/www/static/feed.json"
    },
    {
      "src": "/sitemap.xml",
      "methods": [
        "GET"
      ],
      "dest": "/packages/www/static/sitemap.xml"
    },
    {
      "src": "/atom",
      "methods": [
        "GET"
      ],
      "dest": "/packages/www/static/atom.xml"
    },
    {
      "src": "/(.*)",
      "methods": [
        "GET"
      ],
      "dest": "/packages/www/$1"
    }
  ]
}
