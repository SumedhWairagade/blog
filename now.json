{
  "name": "blog",
  "alias": [
    "mcansh.blog",
    "www.mcansh.blog"
  ],
  "type": "docker",
  "features": {
    "cloud": "v2"
  },
  "env": {
    "TWITTER": "@blog_twitter",
    "INSTAGRAM": "@blog_instagram",
    "GITHUB": "@blog_github",
    "EMAIL": "@blog_email",
    "SENTRY": "@blog_sentry",
    "ANALYTICS": "@blog_analytics",
    "GITHUB_TOKEN": "@blog_github_token"
  }
}
